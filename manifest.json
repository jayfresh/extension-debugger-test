{
  "name": "JavaScript pause/resume",
  "description": "Pauses / resumes JavaScript execution",
  "version": "0.7",
  "permissions": [
    "debugger",
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://*.paperspace.com/*"],
      "js": ["contentScript.js"]
    }
   ],
  "browser_action": {
    "default_icon": "debuggerPause.png",
    "default_title": "Pause JavaScript"
  },
  "manifest_version": 2
}
